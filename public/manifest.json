{
	"name": "南国",
	"version": "1.0.0",
	"description": "支持自定义新标签页的 Chrome 扩展",
	"permissions": ["storage", "tabs"],
	"background": {
		"scripts": ["static/js/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {         
			"16": "icons/16.png",
			"32": "icons/48.png",
			"128":"icons/128.png" 
		}
	},
	"chrome_url_overrides" : {
		"newtab": "index.html"
	},
	"icons": {
		"16": "icons/16.png",
		"32": "icons/48.png",
		"128":"icons/128.png"
	},
	"manifest_version": 2
}